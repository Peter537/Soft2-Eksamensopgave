  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "panels": [],
      "title": "Service Level Objectives (Order Service)",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Procentdel af tid Order Service er up (baseret på Prometheus 'up' over den valgte dashboard-periode)",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 99.0 },
              { "color": "green", "value": 99.9 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 6, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "12.1.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "editorMode": "code",
          "expr": "avg_over_time(up{job=\"order-service\"}[$__range]) * 100",
          "legendFormat": "Uptime",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Order Service Uptime (mål: 99.9%)",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "p95 for order creation latency (fra request-start til Kafka publish er fuldført)",
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 400 },
              { "color": "red", "value": 600 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 6, "x": 6, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "12.1.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "editorMode": "code",
          "expr": "histogram_quantile(0.95, sum(rate(mtogo_order_creation_latency_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "p95",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Order Creation Latency p95 (mål: < 400 ms)",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Andel af CreateOrder-requests der gennemføres uden fejl",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 99.0 },
              { "color": "green", "value": 99.9 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 6, "x": 12, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "12.1.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "editorMode": "code",
          "expr": "((sum(increase(mtogo_order_create_requests_total{result=\"success\"}[$__range])) or vector(0)) / clamp_min((sum(increase(mtogo_order_create_requests_total[$__range])) or vector(0)), 1)) * 100",
          "legendFormat": "Success rate",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Order Creation Success Rate (mål: 99.9%)",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Udnyttelse af kapacitetsmål (1500 req/min). 100% betyder at den aktuelle load matcher 1500 req/min.",
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 6, "x": 18, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "pluginVersion": "12.1.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "editorMode": "code",
          "expr": "(sum(rate(mtogo_order_create_requests_total[1m])) * 60) / 1500 * 100",
          "legendFormat": "Utilization %",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Order Service Capacity Utilization (mål: kan håndtere 1500 req/min)",
      "type": "stat"
    }
  ],
