{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker-net/master/src/Stryker.Core/Stryker.Core/schema.json",
  "stryker-config": {
    "solution": "../../MToGo.sln",
    "project": "MToGo.OrderService.csproj",
    "test-projects": [
      "../../tests/MToGo.OrderService.Tests/MToGo.OrderService.Tests.csproj"
    ],
    "reporters": [
      "html",
      "progress"
    ],
    "report-file-name": "mutation-report",
    "mutate": [
      "Services/**/*.cs"
    ],
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 50
    },
    "concurrency": 2
  }
}
